{
  "expo": {
    "name": "Emovo",
    "slug": "emovo",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "emovo",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEFAE0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luminaryx.emovo"
    },
    "android": {
      "package": "com.luminaryx.emovo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEFAE0"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      "expo-store-review",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Emovo to access your photos to set a profile picture."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#75863C"
        }
      ]
    ]
  }
}
